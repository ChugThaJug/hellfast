{
  "build": {
    "command": "cd frontend && npm install && npm run build",
    "environment": {
      "NODE_VERSION": "18"
    },
    "output_directory": "frontend/build"
  },
  "compatibility_flags": ["nodejs_compat"],
  "routes": [
    { 
      "pattern": "/*", 
      "file": "/index.html", 
      "status": 200 
    }
  ]
}
